
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int page_fault(int pages[] , int n , int c)
{
    int pagefault=0;
    vector<int> v;
    int i;
    int index=0;
    for(int i=0;i<n;i++)
    {
        auto it = std::find(v.begin() , v.end(),pages[i]);
        if(it==v.end())
        {
            if(v.size()==c)
            {
                
                v.erase(v.begin()+(index));
            }
           // v.push_back(pages[i]);
           v.insert(v.begin(),index);
           index++;
           index=index%c;
            pagefault++;
        }
        else
        {v.erase(it);
            v.push_back(pages[i]);
        }
    }
    return pagefault;
}
int main()
{
    int page[]={7,0,1,2,0,3,0,4,2,3,0,3,2};
    int n=13;
    int memocapacity=4;
    cout<<page_fault(page,n,memocapacity)<<endl;
    

    return 0;
}
